{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "MyBGList": "Debug"
    },
    "Console": {
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "UseDeveloperExceptionPage": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost, 1433; MultipleActiveResultSets=true"
  }
}
