{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Debug",
      "MyBGList": "Debug"
    },
    "Console": {
      "FormatterOptions": {
        "SingleLine": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "MyBGList": "Information"
      }
    }
  },
  "UseDeveloperExceptionPage": false,
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost, 1433; MultipleActiveResultSets=true"
  }
}
